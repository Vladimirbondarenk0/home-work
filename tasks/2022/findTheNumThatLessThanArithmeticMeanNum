const findTheNumThatLessThanArithmeticMeanNum = function (arr) {
    if (!Array.isArray(arr)) {
        throw "input parameter is not an array";
    }
    for (let i = 0; i < arr.length; i++) {
            if (typeof(arr[i]) != "number") {
                throw "item with index={" + arr[i] + "}is not a number";
            }
    }
    let sum = 0;
    for (let i = 0; i < arr.length; i++) {
        sum += arr[i];
    }
    let mean = sum / arr.length;
    let result = [];
    for (let i = 0; i < arr.length; i++) {
        if (arr[i] < mean) {
            result.push(arr[i]);
        }
    }
    return result;
};
function testFindTheNumThatLessThanArithmeticMeanNum() {
    
 try {
    findTheNumThatLessThanArithmeticMeanNum();
 }
 catch(error) {
    let res = findTheNumThatLessThanArithmeticMeanNum([3, 3, 1]);
        if (res === "input parameter is not an array") {
            return true;
        } 
