const findTheNumThatLessThanArithmeticMeanNum = function (arr) {
    try {
        if (!Array.isArray(arr)) {
            throw "input parameter is not an array";
        }
    } catch (error) {
        return error;
    }
    try {
        for (let i = 0; i < arr.length; i++) {
            if (typeof (arr[i]) != "number") {
                throw "item with index={" + arr[i] + "}is not a number";
            }
        }
    } catch (error) {
        return error;
    }
    let sum = 0;
    for (let i = 0; i < arr.length; i++) {
        sum += arr[i];
    }
    let mean = sum / arr.length;
    let result = [];
    for (let i = 0; i < arr.length; i++) {
        if (arr[i] < mean) {
            result.push(arr[i]);
        }
    }
    return result;
};

function testFindTheNumThatLessThanArithmeticMeanNum() {
    let res = findTheNumThatLessThanArithmeticMeanNum([1, 3, 3]);
    let arr = [1];
    let comparison = JSON.stringify(res) === JSON.stringify(arr);
    let res1 = findTheNumThatLessThanArithmeticMeanNum("");
    let arr1 = "input parameter is not an array";
    let result;
    if (res1 === arr1){
        result = true;
    } else {
        result = false;
    } 
    if (comparison, result === true)
      return true;
 
}


