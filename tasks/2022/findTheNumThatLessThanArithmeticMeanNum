const findTheNumThatLessThanArithmeticMeanNum = function (arr) {
    let sum = 0;
    for (let i = 0; i < arr.length; i++) {
        sum += arr[i];
    }
    let mean = sum / arr.length;
    let result = [];
    for (let i = 0; i < arr.length; i++) {
        if (arr[i] < mean) {
            result.push(arr[i]);
        }
    }
    return result;
};

function testFindTheNumThatLessThanArithmeticMeanNum() {
    let res = findTheNumThatLessThanArithmeticMeanNum([1, 3]);
    let arr = [1];
    return JSON.stringify(res) === JSON.stringify(arr);

    if (Array.isArray(res) === false) {
        return "input parameter is not an array";
    }                           

    for (let i = 0; i < res.length; i++) {
        if (typeof (res[i]) != "number") {
            return "item with index={" + res[i] + "}is not a number";
        }
    }
}
testFindTheNumThatLessThanArithmeticMeanNum();


